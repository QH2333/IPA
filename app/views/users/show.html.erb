<% provide(:active, 'user') %>
<% content_for :side do %>
  <ul id="first_class">
    <li><a class="first_li active">照片上传</a></li>
  </ul>
<% end %>
<form id="fileupload" action="//jquery-file-upload.appspot.com/" method="POST" enctype="multipart/form-data">
  <div class="row fileupload-buttonbar">
    <div class="large-7 columns">
      <span class="button small fileinput-button">
        <%= fa_icon "plus" %>
        <span>Add files...</span>
        <input type="file" name="files[]" class="button" multiple>
      </span>
      <button type="submit" class="button small start">
        <%= fa_icon "upload" %>
        <span>Start upload</span>
      </button>
      <button type="reset" class="button small cancel">
        <%= fa_icon "ban" %>
        <span>Cancel upload</span>
      </button>
      <button type="button" class="button small delete">
        <%= fa_icon "trash" %>
        <span>Delete</span>
      </button>
      <span class="fileupload-process"></span>
    </div>
    <div class="large-5 columns fileupload-progress fade">
      <div class="progress round">
        <span class="meter" style="width: 0%"></span>
      </div>
    </div>
  </div>
  <table role="presentation" class="table table-striped">
    <tbody class="files"></tbody>
  </table>
</form>
